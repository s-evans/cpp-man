// In header: <boost/random/extreme_value_distribution\&.hpp>



class param_type {
public:
  // types
  typedef extreme_value_distribution distribution_type;

  // construct/copy/destruct
  explicit param_type(RealType = 1\&.0, RealType = 1\&.0);

  // public member functions
  RealType a() const;
  RealType b() const;

