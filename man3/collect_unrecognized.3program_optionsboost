// In header: <boost/program_options/parsers\&.hpp>


template<typename charT> 
  std::vector< std::basic_string< charT > > 
  collect_unrecognized(const std::vector< basic_option< charT > > & options, 
                       enum collect_unrecognized_mode mode);
